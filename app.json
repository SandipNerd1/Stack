{
  "expo": {
    "name": "Pocketstack",
    "slug": "pocketstack",
    "scheme": "pocket",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/App-icon.png",
    "splash": {
      "image": "./assets/logo/NewLogo1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.codechan.pocketstack",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.codechan.pocketstack",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/Appicon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": []
    },
    "web": {
      "favicon": "./assets/Appfavicon.png"
    }
  }
}